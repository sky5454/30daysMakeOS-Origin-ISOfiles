void itoa1()
/* AL -> CL */
{
	CL = AL;
	CL &= 0x0f;
	CL += 0x30; /* '0' */
	if (CL > 0x39)
		CL += 0x41 - 10 - 0x30;
	return;
}

void itoa2()
/* AL -> CX */
{
	itoa1();
	CH = CL;
	ROL(AL, 4);
	itoa1();
	ROL(AL, 4);
	return;
}

void itoa4()
/* AX -> ECX */
{
	itoa2();
	ECX <<= 16;
	XCHG(AL, AH);
	itoa2();
	XCHG(AL, AH);
	return;
}

void itoa8()
/* EAX -> EDX:ECX */
{
	itoa4();
	EDX = ECX;
	ROL(EAX, 16);
	itoa4();
	ROL(EAX, 16);
	return;
}

