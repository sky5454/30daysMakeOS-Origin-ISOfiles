void cmd03c()
/* bit0:drawwindow_back(or drawwindow_frame) + drawwindow() */
/* bit1:drawcomponent() */
/* bit2:option for bit0 */
/* bit3:option diff-mode */
/* bit8:	*/
/* bit9:	*/
{
	WINDOW *win == DS:EDI;
	int *cmd == DS:ESI;

	EAX = cmd[4];
	(offset) win = cmd[8];
	#if (defined(SAFEAPI_FLAG))
		TEST(EAX, 0xfffffcf0);
		if (!= 0) {
			SAFE_CUT("DB ..$-$-1,'windowControl:invalid option: ',0");
		}
	#endif
	TEST(AL, 0x01);
	if (!= 0) {
		// EBP, ESI, EDI以外は全て破壊
		TEST(AL, 0x08);
		if (== 0) {
			TEST(AL, 0x04);
			if (== 0) {
			//	TEST(win->flags, WINFLAG_FRAMEONLY);
			//	if (== 0)
					drawwindow_back();
			//	else
			//		drawwindow_frame();
			/* コンポーネントがウィンドウいっぱいにあるからといって、それでbackを描かないでよいということにはならない。
				アプリはそれを望んでいないかもしれないのだから。だから、bit2をみるべき */
			/* 自分で再描画をやるということは、何でもできるということである */
			} else
				drawwindow_frame();
			drawwindow();
			EAX = cmd[4];
		} else {
			// EBP, ESI, EDI以外は全て破壊
			TEST(win->flags, WINFLAG_TITLE);
			if (!= 0) {
				drawwindow();	/* ウィンドウ影付け,titlebar塗り潰し */
				titlebox_redraw();
				EAX = cmd[4];
			}
		}
	}
	TEST(AL, 0x02);
	if (!= 0) {
		// EBP, ESI, EDI以外は全て破壊
		TEST(AL, 0x08);
		EDX = 3;	/* 強制描画 */
		if (!= 0)
			EDX = 0;	/* 差分描画 */
		drawcomponent();
		EAX = cmd[4];
	}
	TEST(AH, 0x01);
	if (!= 0) {
		PUSH((int) 0);
		PUSH(win->slot);
		PUSH((int) 0x0140);
		EBX = ESP;
		CALL(0x0017,0); // GUIGUI統合shellcall
		ESP += 12;
	}
	TEST(AH, 0x02);
	if (!= 0) {
		PUSH((int) 0);
		PUSH(win->slot);
		PUSH((int) 0x0144);
		EBX = ESP;
		CALL(0x0017,0); // GUIGUI統合shellcall
		ESP += 12;
	}
	(offset) cmd += 12;
	asmout("JMP nextcmd");
}

void cmd0a0()
/* ファイルハンドル初期化 */
{
	int *cmd == DS:ESI;
	EAX = cmd[ 4];
	PUSH((int) 0);
	if ((unsigned) EAX < 0xe0) {
		PUSH(cmd[ 8] /* slot */);
		PUSH(EAX /* opt */);
		PUSH((int) 0x0700);
		(offset) cmd += 12;
		EBX = ESP;
		CALL(0x0017,0); // shell-call
		ESP += 16;
		asmout("JMP nextcmd");
	}
	/* サブコマンド付き */
	/* cmd[12]はサブコマンド総バイト数 */
	PUSH(cmd[20] /* sel */);
	PUSH(cmd[16] /* ofs */);
	PUSH(cmd[12] /* bytes */);
	PUSH(cmd[ 8] /* slot */);
	PUSH((int) EAX);
	PUSH((int) 0x0700);
	(offset) cmd += 24;
	EBX = ESP;
	CALL(0x0017,0); // shell-call
	ESP += 28;
	asmout("JMP nextcmd");
}

