void signal_cut_mode()
{
	WORK *work == SS:EBP;
	int *cmd == DS:ESI;

	PUSHAD();
	EAX = 0;

	/* 溜っているシグナルをもみ消す */
	work->signalbox_read_free = EAX;
	/* フラグに記録, 以後シグナルは握りつぶされアプリに制御が渡らない */
	work->signalbox_status |= SAFEAPI_FLAG;

	/* メッセージ */
	EBP = ESP;
	EBX = [SS:EBP + 4*8];			/* ret-eip */
	EDX = (unsigned char) [CS:EBX];	/* message length */
	EDI = EDX;
	do {
		AL = [CS:EBX+1 +EDX-1]; /* 後ろから1文字ずつ読む */
		if ((unsigned) AL < 16) {
			EDI += 7;
			EAX *= 4;
			ECX = 0;
			if ((unsigned) AL < 8*4) {
				AL ^= 7*4;
				do {						/* レジスタダンプ */
					CL = 0x0f;
					CL &= [SS:EBP+EAX];
					asmout("MOV CL,BYTE [CS:hextable+ECX]");
					PUSH(ECX);
					CL = [SS:EBP+EAX];
					ECX >>= 4;
					asmout("MOV CL,BYTE [CS:hextable+ECX]");
					EAX++;
					PUSH(ECX);
					TEST(AL,3);
				} while(!=0);
			} else {	/* コマンドダンプ */
				do {
					CL = 0x0f;
					CL &= [DS:ESI+EAX-8*4];
					asmout("MOV CL,BYTE [CS:hextable+ECX]");
					PUSH(ECX);
					CL = [DS:ESI+EAX-8*4];
					ECX >>= 4;
					asmout("MOV CL,BYTE [CS:hextable+ECX]");
					PUSH(ECX);
					EAX++;
					TEST(AL,3);
				} while (!=0);
			}
		} else
			PUSH(EAX); /* 通常文字 */
		EDX--;
	} while (!=0);

	EBX = ESP;
	PUSH(EDX);			/* eoc */
	PUSH(SS);
	PUSH(EBX);
	PUSH(EDI);			/* length */
	PUSH(EDX);			/* base */
	PUSH(0xc0);			/* ascii */
	PUSH(EDX);			/* backcolor */
	PUSH(15);			/* color */
	PUSH(EDX);			/* y */
	PUSH(EDX);			/* x */
	PUSH(EDX);			/* no slot for window */
	PUSH(EDX);			/* opt */
	PUSH(0x0404);			/* put text */
	EBX = ESP;
	CALL(0x0017,0);		/* GUIGUI統合shellcall */

	/* waitsignalコマンドを捏造, 永眠せよ */
	cmd[0] = 0x18;		/* wait signal */
	cmd[4] = 0x01;
	cmd[8] = EDX;
	cmd[12] = EDX;
	cmd[16] = EDX;		/* end of command */

	ESP = EBP;
	POPAD();
	asmout("JMP nextcmd");
}
