	/*	DB( 0, 0,-1, 0);	*/			/* End of data */
	closebutton:
		DD(0x0f0f0f0f, 0x0f0f0f0f, 0x0f0f0f0f, 0x000f0f0f);	/* ×箱(^^; */
		DD(0x0808080f, 0x08080808, 0x08080808, 0x00070808);
